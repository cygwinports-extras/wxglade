inherit python-distutils

NAME="wxglade"
VERSION=0.6.5
RELEASE=1
CATEGORY="Devel"
SUMMARY="wxWidgets GUI builder"
DESCRIPTION="wxGlade is a GUI designer written in Python with the popular GUI
toolkit wxPython, that helps you create wxWidgets/wxPython user interfaces. At
the moment it can generate Python, C++, Perl, Lisp and XRC code."
HOMEPAGE="http://wxglade.sourceforge.net/"
SRC_URI="mirror://sourceforge/${PN}/wxGlade-${VERSION}.tar.gz"
SRC_DIR="wxGlade-${VERSION}"
